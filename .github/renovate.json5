{
  baseBranches: [
    "main",
    "/^release\\/.*/",
  ],
  enabledManagers: [
    "regex",
  ],
  regexManagers: [
    {
      fileMatch: ["^Dockerfile$"],
      matchStrings: ["ARG TOOLS_VERSION=(?<currentValue>.*?)\\n"],
      datasourceTemplate: "git-tags",
      depNameTemplate: "https://github.com/nedix/kubernetes-tools-docker.git",
      versioningTemplate: "semver-coerced"
    },
  ],
}
