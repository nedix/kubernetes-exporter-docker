{
  baseBranches: [
    "main",
    "/^release\\/.*/",
  ],
  enabledManagers: [
    "regex",
  ],
  regexManagers: [
    {
      fileMatch: ["^Dockerfile$"],
      matchStrings: ["ARG TOOLS_VERSION=(?<currentValue>.*?)\\n"],
      datasourceTemplate: "github-releases",
      depNameTemplate: "nedix/kubernetes-tools-docker"
    },
  ],
}
